<!DOCTYPE html>
<html>
	<% include head %>
	<body>
		<div class="container">
			<div class="row top30">
				<div class="col col-md-5 col-md-offset-3">
					<div class="panel">
						<div class="panel-heading">Entrar</div>
						<div class="panel-body">
							<div class="row">
								<div class="col col-md-12">
									<form action="/logar" method="POST">
										<input type="text" class="form-control" name="nome" placeholder="usuario">
										<input type="password" class="form-control" name="senha" placeholder="senha">
										<button type="submit" id="btnLogar" class="btn btn-primary">Logar</button>

										<a href="/cadastro" class="btn btn-simple btn-info">Cadastrar</a>
									</form>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</body>
</html>
